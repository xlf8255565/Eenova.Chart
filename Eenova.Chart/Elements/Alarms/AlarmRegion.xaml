<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Eenova.Chart.Elements">

    <Style TargetType="local:AlarmRegion">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:AlarmRegion">
                    <ContentPresenter Content="{TemplateBinding RegionHost}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>
