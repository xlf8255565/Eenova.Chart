<local:BaseSetter x:Class="Eenova.Chart.Setter.AxisTextFormatSetter"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                  xmlns:local="clr-namespace:Eenova.Chart.Setter"
                  mc:Ignorable="d" x:Name="Root"
                  d:DesignHeight="300" d:DesignWidth="400">

    <Grid Margin="5" Background="White">
        <local:TextFormatSelector x:Name="formater"
                                  Format="{Binding Format,Mode=TwoWay,UpdateSourceTrigger=Explicit}"/>
    </Grid>
</local:BaseSetter>
