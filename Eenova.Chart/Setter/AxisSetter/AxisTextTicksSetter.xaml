<local:BaseSetter x:Class="Eenova.Chart.Setter.AxisTextTicksSetter"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                  xmlns:c="clr-namespace:Eenova.Chart.Controls"        
                  xmlns:local="clr-namespace:Eenova.Chart.Setter"
                  mc:Ignorable="d"
                  d:DesignHeight="300" d:DesignWidth="400">

    <c:GroupBox Margin="5" Title="文本轴刻度">
        <CheckBox x:Name="cbIsDesc" Content="文本次序反转" Margin="10,10,0,0" VerticalAlignment="Top" HorizontalAlignment="Left"
                  IsChecked="{Binding IsDesc, Mode=TwoWay,UpdateSourceTrigger=Explicit}"/>
    </c:GroupBox>

</local:BaseSetter>
